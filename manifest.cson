# Use cson2json from cson (npm) to convert this (Emacs does it for me).
manifest_version: 2
name: 'Slack Emoji'
version: '0.1.5'
description: 'Make it easier to add emoji to slack. Sets the name to be the same as the filename and focuses the submit button.'
content_scripts: [
  matches: ["*://*.slack.com/*"]
  js: ["slackemoji.js"]
  css: ["slackemoji.css"]
]
permissions: [
  "*://*.slack.com/*"
]
icons:
  16: "icon16.png"
  48: "icon48.png"
  128: "icon128.png"
